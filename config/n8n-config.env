N8N_DIAGNOSTICS_ENABLED=false
N8N_HIRING_BANNER_ENABLED=false
EXECUTIONS_MODE=regular
N8N_LOG_LEVEL=info
N8N_METRICS=false
# Disable the built-in Python task runner — this workflow uses only
# JavaScript Function nodes, and the slim n8n Docker image does not
# ship Python 3.  Without this flag n8n tries to start a Python runner
# in internal mode, which fails with:
#   "Failed to start Python task runner in internal mode …
#    because Python 3 is missing from this system"
#   '"--id" has to be set!'
N8N_RUNNERS_DISABLED=true
# Pieces OS local API (macOS/Windows: 1000, Linux: 5323)
PIECES_OS_BASE_URL=http://localhost:1000
