N8N_DIAGNOSTICS_ENABLED=false
N8N_HIRING_BANNER_ENABLED=false
EXECUTIONS_MODE=regular
N8N_LOG_LEVEL=info
N8N_METRICS=false
# Prevent n8n from trying to start a Python task runner internally.
# The slim n8n Docker image does not ship Python 3, which causes:
#   "Failed to start Python task runner in internal mode â€¦
#    because Python 3 is missing from this system"
#   '"--id" has to be set!'
# This workflow only uses JavaScript Function nodes, so Python is unnecessary.
N8N_PYTHON_ENABLED=false
N8N_RUNNERS_MODE=external
# Pieces OS local API (macOS/Windows: 1000, Linux: 5323)
PIECES_OS_BASE_URL=http://localhost:1000
